                            "type": "object"
                        },
                        "rest": {
                            "properties": {
                                "function_name": {
                                    "type": "string"
                                },
                                "parameters": {
                                    "$ref": "#/definitions/transformation.options.gloo.solo.io.Parameters",
                                    "additionalProperties": true
                                },
                                "response_transformation": {
                                    "$ref": "#/definitions/envoy.api.v2.filter.http.TransformationTemplate",
